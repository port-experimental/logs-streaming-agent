# CI/CD Provider Configuration

# Jenkins Configuration
JENKINS_URL=http://localhost:8080
JENKINS_USERNAME=your-username
JENKINS_API_TOKEN=your-api-token
JENKINS_JOB_NAME=your-node-app

# Webhook Server Configuration
WEBHOOK_PORT=3000

# Port.io Configuration
PORT_CLIENT_ID=your_port_client_id_here
PORT_CLIENT_SECRET=your_port_client_secret_here
PORT_ORG_ID=org_your_org_id_here

# Kafka Configuration
KAFKA_BROKERS=broker1.kafka.port.io:9092,broker2.kafka.port.io:9092,broker3.kafka.port.io:9092
KAFKA_USERNAME=your_kafka_username
KAFKA_PASSWORD=your_kafka_password
KAFKA_CONSUMER_GROUP_ID=org_your_org_id_here.my-consumer-group

# Logging Configuration
LOG_LEVEL=info
NODE_ENV=development